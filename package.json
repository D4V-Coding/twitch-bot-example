{
  "name": "Twitch.Example.Bot",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/startup.js"
  },
  "private": true,
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "jsxBracketSameLine": true,
    "htmlWhitespaceSensitivity": "strict",
    "quoteProps": "as-needed",
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 160
  },
  "dependencies": {
    "@types/node": "^14.14.2",
    "tmi.js": "^1.5.0",
    "tslint": "^6.1.3"
  },
  "devDependencies": {
    "@types/tmi.js": "^1.4.2",
    "tslint": "^6.1.3",
    "typescript": "^4.0.5"
  }
}
